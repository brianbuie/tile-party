{
  "name": "tile-party",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:server": "nodemon --exec node_modules/.bin/babel-node -r dotenv/config api/server.js",
    "dev:client": "sleep 3 && vite",
    "dev": "concurrently yarn:dev:* -p none",
    "manifest": "babel-node build-manifest.js",
    "analyze": "react-scanner -c analysis.config.js",
    "stage": "yarn build && babel-node -r dotenv/config api/server.js dotenv_config_path=.env.prod",
    "build": "vite build --emptyOutDir",
    "start": "babel-node api/server.js"
  },
  "engines": {
    "node": "17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.7",
    "@babel/node": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@vitejs/plugin-react": "^1.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^6.2.1",
    "icon-gen": "^3.0.1",
    "nodemon": "^2.0.13",
    "passport-mock-strategy": "^2.0.0",
    "prop-types": "^15.7.2",
    "react-scanner": "^1.0.3",
    "rimraf": "^3.0.2",
    "vite": "^2.5.10"
  },
  "dependencies": {
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "framer-motion": "^6.5.1",
    "helmet": "^5.1.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.0",
    "passport": "0.5",
    "passport-facebook": "^3.0.0",
    "path-parser": "^6.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "use-http": "^1.0.26"
  }
}
