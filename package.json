    "vite": "^2.5.10"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "framer-motion": "^6.5.1",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
